{
    "METADATA_SPEC_VERSION": "SINGLE_TABLE_V1",
    "primary_key": "SECURITY_ID",
    "columns": {
        "SECURITY_ID": {
            "sdtype": "id"
        },
        "TICKER": {
            "sdtype": "id",
            "regex_format": "[A-Z]{5,7}$"
        },
        "SECURITY_NAME": {
            "sdtype": "categorical"
        },
        "SECTOR": {
            "sdtype": "categorical"
        },
        "INDUSTRY":{
            "sdtype":"categorical"
        },
        "MARKET_CAP_USD": {
            "sdtype": "numerical"
        },
        "MATURITY_DATE": {
            "sdtype": "datetime",
            "datetime_format": "%d-%m-%Y"
        },
        "CURRENT_PRICE": {
            "sdtype": "id",
            "regex_format": "[1-9][0-9][0-9]\\.\\d{2}$"
        },
        "LISTING_EXCHANGE": {
            "sdtype": "categorical"
        },
        "COUNTRY": {
            "sdtype": "categorical"
        }
    },
    "constraint_class": "FixedCombinations",
    "constraint_parameters": {
        "column_names": ["SECTOR", "INDUSTRY","SECURITY_NAME"]
    }
}
